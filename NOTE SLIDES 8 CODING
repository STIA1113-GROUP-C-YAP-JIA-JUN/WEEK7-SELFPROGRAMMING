package myWhile;

public class NoteSlides8 {

	public static void main(String[] args) {
		
		int i;
		int j; 
		int k;
		
		for (i = 1; i < 5; i++) {
			System.out.println(i + "");
		}
		System.out.println();
		
		for (j = 10; j < 20; j++) {
			System.out.println(j + "");
		}
		System.out.println();
		
		for (k = 20; k > 10; k--) {
			System.out.println(k + "");
		}
		System.out.println();
		
