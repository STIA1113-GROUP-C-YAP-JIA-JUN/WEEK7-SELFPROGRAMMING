package myWhile;

public class NoteSlides7 {

	public static void main(String[] args) {
		
		int i = 1;
		int j = 10;
		int k = 20;
		
		do {
			System.out.print(i + "");
			i ++;
		}while (i < 5);
		System.out.println();
		
		do{
			System.out.println(j + "");
			j ++;
		}while (j < 20);
		System.out.println();
		
		do {
			System.out.println(k + "");
			k --;
		}while (k > 10);
