package myWhile;

public class NoteSlides6 {

	public static void main(String[] args) {
		
		int i = 1;
		int j = 10;
		int k = 20;
		
		while (i<5) {
			System.out.println(i + "");
			i++;
		}
		System.out.println();
		
		while (j<20) {
			System.out.println(j + "");
			j = j + 1;
		}
		System.out.println();
		
		while (k>10) {
			System.out.println(k + "");
			k--;
		}
